<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
   <system.serviceModel>
      <behaviors>
         <serviceBehaviors>
            <behavior name="MyBehav">
               <serviceMetadata httpGetEnabled="true" />
               <serviceDebug includeExceptionDetailInFaults="true" />
            </behavior>
         </serviceBehaviors>
      </behaviors>
      <services>
         <service behaviorConfiguration="MyBehav" name="PubSubLib.PubSub">
            <endpoint address="PS" binding="wsDualHttpBinding" bindingConfiguration="" name="PSEP" contract="PubSubLib.ILongCompute" />
            <host>
               <baseAddresses>
                  <add baseAddress="http://localhost:7060/" />
               </baseAddresses>
            </host>
         </service>
      </services>
   </system.serviceModel>
</configuration>