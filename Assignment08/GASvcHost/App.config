<?xml version="1.0" encoding="utf-8" ?>
<configuration>
   <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
   </startup>
   <system.serviceModel>
      <behaviors>
         <serviceBehaviors>
            <behavior name="GABehavior">
               <serviceMetadata httpGetEnabled="true" />
               <serviceDebug includeExceptionDetailInFaults="true" />
            </behavior>
         </serviceBehaviors>
      </behaviors>
      <services>
         <service behaviorConfiguration="GABehavior" name="GASvcLib.GASvc">
            <endpoint address="GA" binding="wsDualHttpBinding" bindingConfiguration="" name="GASV" contract="GASvcLib.IGASvc" />
            <host>
               <baseAddresses>
                  <add baseAddress="http://localhost:7060/" />
               </baseAddresses>
            </host>
         </service>
      </services>
   </system.serviceModel>
</configuration>